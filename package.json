  "bugs": {
    "url": "https://github.com/letItBezhumphries/mern-proshop/issues"
  },
  "homepage": "https://github.com/letItBezhumphries/mern-proshop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.9",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^7.1.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15"
